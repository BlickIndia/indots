/* slider css */
header {
	min-height 100%
	color $white
	width 100%
	
	.header-content {
		position relative
		text-align left
		width 100%
	  	
	  	.header-content-inner {
			width 30%
			margin-left 680px
		}
		
		.carousel-inner {
			
			.carousel-caption p{
				font-size 22px
				margin-left auto
				margin-right auto
				margin-bottom 75px
			}
		}	
	}
}
.carousel-control {
	width 4% !important
}
.carousel-indicators {
	display none 
}
.head_text_first{
	margin-bottom 50px !important
}
.carousel-control.left, .carousel-control.right {
   background-image none !important
   filter none !important
}
.carousel-caption {
	text-align left
	text-shadow 0px 0px 0px !important
	position absolute
	bottom 160px
	left 15%
}
.carousel-caption.caption_align {
	position absolute
	bottom 55px
	left 15%
}
h1.banner_text {
	color $blue
	font-size 44px
	font-family 'Avenir LT Std 85 Heavy'
}
.first_banner{
	margin-bottom 10px
}
bg-primary.btn-xl {
	padding 14px 25px !important
	color $white
	font-weight 900
	background-color $blue !important
	font-size 18px !important
	font-family 'Avenir LT Std 85 Heavy'
}
.glyphicon {
    padding 14px
    background-color $blue
    color $white
    border-radius 40px
    .glyph_l, .glyph_2 {
		height 28px
		width 28px
	}
}
.glyphicon.glyph_right {
	position relative
	top 86%
	right 68%
}
.glyphicon.glyph_left {
	position relative
	top 86%
	left 68%
}
.social_img {
    opacity 1
    transition 0.5s ease
}
.social_img:hover {
    transform scale(1.5, 1.5)
    opacity 1
    transition 0.5s ease
}
.about_text {
	color $darkgrey
	font-weight 500
	font-size 18px
	margin-bottom 30px
}
.blog_text {
	margin-bottom 70px
}
.blog{
	padding 50px 0
}
.text_caption_thumb{
	margin-bottom 0px
	line-height 18px
}
section {
	padding 100px 0
}
.btn-xl {
	padding 14px 25px
	color $white
	font-weight 900
	background-color $blue
	font-size 18px
}
.btn.btn-default.btn-xl-more {
	font-size 18px
	padding 0px 28px
	font-family 'Avenir LT Std 85 Heavy'
	height 62px
}
.bg-primary {
	/* background-image url(/img/bg-thumb/rectangle-4@3x.png) */
	background-size cover
	padding-bottom 0px
	height auto
}
.bg-primary.parallax {
	background-image url(/img/bg-thumb/rectangle-4@3x.png)
	}
.text-center.logo-center {
	margin-bottom 50px
}
.custom_container {
	relative()
	top -165px
}
.text_s2 {
	margin-top 30px
	font-size 16px
}
.sr-icons {
	height 60px
}
.blog_content {
	h2.blog_title {
		color $darkgrey
		margin-bottom 40px
		font-family 'Avenir LT Std 85 Heavy'
	}
}
.blog_content p {
 	color $darkgrey
}

.img.img-responsive.thumb-img {
	width 410px
}

.thumbnails {
	padding-top 0px
}
.thumbnail {
	width 410px
	padding 0 !important
	background-color $lightgray !important
	border 0px none !important
	height 460px
	.caption{
		p{
			color $darkgrey
		}
	}
	.cap_para {
	    font-size 16px
	}
	.cap_para2 {
	    font-size 14px
	}
}
.thumb_cap {
	width 100%
}
.subscribe{
	margin-top 50px
}
.user_img {
	width 22%
	float left
	position relative
	top -45px
	left 14px
}
.thumb_quote {
	width 60%
	float left
	margin-left 32px
	margin-top 5px
	font-family 'Avenir LT Std 45 Book'
}
.thumb_s1 {
	padding-left 95px !important
	padding-right 95px !important
	color $darkgrey
}
.thumb_text {
	font-size 22px
	line-height 22px
	margin 75px 0
}
.head_thumb {
	padding-top 10px
	font-size 20px
}
.content_flash {
	padding 0px 0px !important
}
.content_head {
	color $blue
	margin-top 44px !important
	font-family 'Avenir LT Std 85 Heavy'
}
.content_title {
	color $darkgrey
	font-size 22px
	font-weight 700
}
.col-lg-3.col-md-3.sub_section {
	text-align center
}
.border_head {
	border-top .5px solid $darkwhite
}
.sub_section_detail {
	margin-top 80px
}
.icon {
	width 38px
	height 45px
}
.icon2 {
	height 55px
}
.icon4 {
	width 30px
}
.text_margin {
	margin-top 25px
}
.text_margin2 {
	margin-top 15px
}
.thumbnail.tn_home {
	width 345px
	margin-bottom 50px !important
	height 424px
	.caption {
		padding 18px 83px !important
	}
	.caption.last{
		padding 31px 83px !important
	}
}
.btn1.btn-xl-more {
	padding 12px 55px
	color #fff
	font-weight 500
	background-color $btncolor
	font-size 18px
	border 0px none
	border-radius 4px
	font-family 'Avenir LT Std 85 Heavy'
	a{
		color #fff
	}
}
.btn1.btn-xl-more:hover {
	background-color $blue
}
.more_btn {
	text-align center
}
.form-control {
	height 62px !important
	border 0.5px solid $darkwhite !important
	border-radius 0px !important
	width 325px !important
	padding 14px 50px
	font-size 18px
}
.form-inline {
	text-align center
}
.btn-default {
	color $white !important
	background-color $btngreen !important
	border none
}
.form-group {
	padding 0px 8px
}
.sub_caption {
	font-family 'Avenir LT Std 45 Book Oblique'
}
.sub_caption.cap_1 {
	position absolute
	bottom 40px
	left 137px
	color $grey1 !important
	font-size 12px
	right 132px
}
.sub_caption.cap_2 {
	position absolute
	bottom 20px
	left 137px
	color $grey1 !important
	font-size 12px
	right 132px
}

@media only screen and (max-width: 768px) {
	.thumb_s1{
		padding 0px 0px !important
	}
	.thumbnail.tn_home {
		width auto
		height auto
	}
	.thumbnail {
		width auto
		height 440px
		margin 12px 30px
	}
	.sub_caption.cap_1, .sub_caption.cap_2 {
		position absolute
		left 40px
		right 39px
	}
	.sub_caption.cap_1 {
		bottom 8px
	}
	.sub_caption.cap_2 {
		bottom 0px
	}
	.form-control {
		width 100% !important
	}
	.white_text {
		margin-left 0px
	}
	.footer_form {
		text-align left
	}
	.glyphicon{
	    padding 4px
	    border-radius 40px
	    .glyph_l, .glyph_2 {
			height 17px
			width 23px
		}
	}
	.glyphicon.glyph_right {
		position relative
		top 97px
		right 55px
	}
	.glyphicon.glyph_left {
		position relative
		top 98px
		left 36px
	}
	.about {
		padding-bottom 0px
	}
	#myCarousel {
		margin-top: 58px;
	}
}

@media only screen and (min-width: 769px) and (max-width: 990px) {
	.hm_thumbnail {
		padding 0px 77px
	}
	.thumb_s1 {
		padding-left 0px !important
		padding-right 0px !important
		width 390px
		float left
	}
	.thumbnail {
		height 463px
		width 380px
		float left
	}
	.sub_caption.cap_1, .sub_caption.cap_2 {
		position absolute
		left 80px
		right 80px
	}
	.sub_caption.cap_2 {
		bottom 30px
	}
	.left_widget, .right_widget {
		width 50%
		float left
	}
	.white_text {
		margin-left 0px
	}
	.form-control1 {
		width 245px !important
	}
	.hm_thumbnail {
		padding 0 23px
	}
}

@media only screen and (min-width: 991px) and (max-width: 1280px){
	.hm_thumbnail {
		padding 0px 77px
	}
	.thumb_s1 {
		padding-left 45px !important
		padding-right 45px !important
	}
	.thumbnail {
		height 480px
	}
	.sub_caption.cap_1, .sub_caption.cap_2 {
		position absolute
		left 80px
		right 80px
	}
	.sub_caption.cap_2 {
		bottom 30px
	}
}
/* thumbnail */
@media only screen and (min-width: 480px) and (max-width: 769px){
	.user_img {
		width 23%
		position relative
		left 25px
	}
	.thumbnail {
		height 470px
		margin 0px auto
		width 410px
	}
	.thumb_s1{
		margin-bottom 20px
	}
	.sub_caption.cap_1, .sub_caption.cap_2 {
		position absolute
		left 217px
		width 292px
	}
}
/* slider responsive */
@media only screen and (max-width: 434px){
	header {
		.header-content{
			.header-content-inner {
				position absolute 
				right 0px 
				width 142px 
				left auto 
				max-width 100% 
				top 102px
			}
			.header-content-inner.first {
				position absolute
				top 72px !important
			}
			.carousel-inner{
				.carousel-caption{
					p{
						font-size 8px 
						margin-bottom 0px 
					}
					.head_text_first{
						margin-bottom 0px !important
					}
				}
			}
		}
	} 
	h1.banner_text{
		font-size 10px !important
		margin-bottom 0px !important
	}
	.btn-xl.slider_btn {
		padding 4px 7px !important
		font-size 10px !important
	}
}
@media only screen and (min-width: 435px) and (max-width: 769px){
header {
		.header-content{
			.header-content-inner {
				position absolute
				right 0px
				width 220px
				left auto
				max-width 100%
				top 132px
			}
			.header-content-inner.first {
				position absolute
				top 100px !important
			}
			.carousel-inner{
				.carousel-caption{
					p{
						font-size 10px
						margin-bottom 5px 
					}
					.head_text_first{
						margin-bottom 0px 
					}
				}
			}
		}
	} 
	h1.banner_text{
		font-size 14px
	}
	.btn-xl.slider_btn {
		padding 7px 10px
		font-size 12px
	}
	.first_banner{
		margin-bottom 0px
	}
}
@media only screen and (min-width: 769px) and (max-width: 990px){
	header {
		.header-content{
			.header-content-inner {
				position absolute
				right 0px
				width 350px
				left auto
				max-width 100%
				top 123px
			}
			.header-content-inner.first {
				position absolute
				top 76px 
			}
			.carousel-inner{
				.carousel-caption{
					p{
						font-size 18px
						margin-bottom 42px 
					}
					
				}
			}
		}
	} 
	h1.banner_text{
		font-size 33px
	}
	.btn-xl.slider_btn {
		padding 10px 16px
	}
}
@media only screen and (min-width: 991px) and (max-width: 1366px){
	header {
		.header-content{
			.header-content-inner {
				position absolute
				right 0px
				width 430px
				left auto
				max-width 100%
				top 178px
			}
			.header-content-inner.first {
				position absolute
				top 130px 
			}
			.carousel-inner{
				.carousel-caption{
					p{
						font-size 18px
						margin-bottom 42px 
					}
					.head_text_first{
						margin-bottom 10px !important
					}
				}
			}
		}
	} 
	h1.banner_text{
		font-size 33px
	}
	.btn-xl.slider_btn {
		padding 10px 16px
	}
}